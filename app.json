{
  "expo": {
    "name": "TechVlog",
    "slug": "TechVlog",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/techicon.png",
    "splash": {
      "image": "./assets/techicon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/techicon.png",
        "backgroundColor": "#FFFFFF"
      },
      "useNextNotificationsApi": true,
      "googleServicesFile": "./gglserve.json",
      "package": "com.nathu.TechVlog"
    },
    "notification": {
      "icon": "./assets/techicon.png",
      "color": "#311b92",
      "androidMode": "default",
      "androidCollapsedTitle": "TechVlog"
    },
    "web": {
      "favicon": "./assets/techicon.png"
    }
  }
}